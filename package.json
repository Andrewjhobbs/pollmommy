    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-teamcity-reporter": "^1.1.1",
    "standard": "^8.0.0",
    "testdouble": "^1.9.0"
  },
  "scripts": {
    "test": "node_modules/.bin/eslint . && node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive test",
    "coverage": "./node_modules/.bin/istanbul cover --report lcovonly test/* && ./node_modules/.bin/codecov",
    "clean": "rm -rf {bin,lib}",
    "compile": "mkdir -p {bin,lib} && ./node_modules/.bin/babel -o ./lib/index.js ./src/pollmommy.js && ./node_modules/.bin/babel -o ./lib/user-agent.js ./src/user-agent.js && ./node_modules/.bin/babel -o bin/pollmommy.js src/app.js",
