  "dependencies": {
    "bluebird": "^3.4.6",
    "commander": "^2.9.0",
    "lodash": "^4.17.2",
    "nightmare": "^2.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codecov": "^1.0.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-mocha": "^4.3.0",
    "eslint-plugin-node": "^3.0.5",
